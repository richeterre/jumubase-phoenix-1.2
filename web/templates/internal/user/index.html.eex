<h2><%= gettext("Users") %></h2>

<table class="table">
  <thead>
    <th><%= gettext("Name") %></th>
    <th><%= gettext("Email") %></th>
  </thead>
  <tbody>
    <%= for user <- @users do %>
      <tr>
        <td><%= "#{user.first_name} #{user.last_name}" %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link gettext("Add User"), to: internal_user_path(@conn, :new), class: "btn btn-primary" %>
